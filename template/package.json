{
  "name": "<gany.APP_PACKAGE_NAME>",
  "version": "<gany.APP_PACKAGE_VERSION>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --disable-host-check",
    "build": "npm run clean-dist; ng build --prod --vendor-chunk",
    "server": "tsc ganymede.app.server.ts; node ganymede.app.server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw-update": "cp src/app/ganymede/components/util/sw/sw.ts src/assets-root/sw.ts; tsc src/assets-root/sw.ts;",
    "electron-entry": "mkdir -p dist-electron; cp src/app/ganymede/electron/* .",
    "electron-start": "npm run electron-entry && electron . start",
    "electron-test": "npm run electron-entry && electron . test",
    "electron-build-mac": "npm run build && npm run electron-entry; electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/assets/ico/mac/icon.icns --prune=true --out=dist-electron --dir=dist-electron --ignore='(src*|profiles*|node_modules*|ganymede.app.server*)'",
    "clean-dist": "rm -rf dist/*; rm -rf dist-electron/*; rm -rf dist-server/*;",
    "postinstall": "npm run sw-update;",
    "product-name": "node -p 'try{require(`./ganymede.conf.json`).productName}catch(e){``}'",
    "version": "echo $npm_package_version"
  },
  "private": true,
  "main": "electron.js",
  "dependencies": {
    "@angular/animations": "^11.2.9",
    "@angular/common": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/platform-browser": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@cds/core": "^5.1.1",
    "@clr/angular": "^5.1.1",
    "@clr/core": "^4.0.12",
    "@clr/icons": "^5.1.1",
    "@clr/ui": "5.1.1",
    "@jovian/fourq": "^1.0.11",
    "@jovian/langster": "0.0.6",
    "@jovian/type-tools": "^6.0.5",
    "@ngrx/store": "^11.1.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "ngx-markdown": "^11.1.2",
    "rxjs": "~6.6.7",
    "typescript": "^4.2.4",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.8",
    "@angular/cli": "~11.2.8",
    "@angular/compiler": "^11.2.9",
    "@angular/compiler-cli": "^11.2.9",
    "@types/jasmine": "~3.6.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.37",
    "codelyzer": "^6.0.1",
    "electron": "^12.0.2",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslib": "^2.2.0",
    "tslint": "~5.20.1"
  }
}
